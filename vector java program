import java.util.*;
class vector
{
 public static void main(String args[])
 {
  Vector v=new Vector();
  v.add(101);
  v.add("san");
  v.add(6.58);
  v.add(102);
  v.add("arha");
  v.add(8.98);
  System.out.println(v);
  v.remove(4);
  System.out.println(v);
  v.set(2,9.17);
  System.out.println(v);

 v.contains("rohi");
 System.out.println(v.contains("rohi"));
}
}
